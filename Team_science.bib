@article{bovillAddressingPotentialChallenges2016,
  title = {Addressing Potential Challenges in Co-Creating Learning and Teaching: Overcoming Resistance, Navigating Institutional Norms and Ensuring Inclusivity in Student–Staff Partnerships},
