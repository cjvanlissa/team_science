@article{guiseTeamMentoringInterdisciplinary2017,
  title = {Team {{Mentoring}} for {{Interdisciplinary Team Science}}: {{Lessons}} from {{K12 Scholars}} and {{Directors}}},
  shorttitle = {Team {{Mentoring}} for {{Interdisciplinary Team Science}}},
  author = {Guise, Jeanne-Marie and Geller, Stacie and Regensteiner, Judith G. and Raymond, Nancy and Nagel, Joan},
  date = {2017-02},
  journaltitle = {Academic medicine : journal of the Association of American Medical Colleges},
  shortjournal = {Acad Med},
