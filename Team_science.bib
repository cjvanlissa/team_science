@article{hallFourphaseModelTransdisciplinary2012,
  title = {A Four-Phase Model of Transdisciplinary Team-Based Research: Goals, Team Processes, and Strategies},
  shorttitle = {A Four-Phase Model of Transdisciplinary Team-Based Research},
  author = {Hall, Kara L. and Vogel, Amanda L. and Stipelman, Brooke A and Stokols, Daniel and Morgan, Glen and Gehlert, Sarah},
  date = {2012-12-01},
  journaltitle = {Translational Behavioral Medicine},
  shortjournal = {Translational Behavioral Medicine},
  volume = {2},
  number = {4},
  pages = {415--430},
  issn = {1869-6716},
  doi = {10.1007/s13142-012-0167-y},
  url = {https://doi.org/10.1007/s13142-012-0167-y},
  urldate = {2023-12-03},
  abstract = {The complexity of social and public health challenges has led to burgeoning interest and investments in cross-disciplinary team-based research, and particularly in transdisciplinary (TD) team-based research. TD research aims to integrate and ultimately extend beyond discipline-specific concepts, approaches, and methods to accelerate innovations and progress toward solving complex real-world problems. While TD research offers the promise of novel, wide-reaching, and important discoveries, it also introduces unique challenges. In particular, today's investigators are generally trained in unidisciplinary approaches and may have little training in, or exposure to, the scientific skills and team processes necessary to collaborate successfully in teams of colleagues from widely disparate disciplines and fields. Yet these skills are essential to maximize the efficiency and effectiveness of TD team-based research. In the current article, we propose a model of TD team-based research that includes four relatively distinct phases: development, conceptualization, implementation, and translation. Drawing on the science of team science field, as well as the findings from previous research on group dynamics and organizational behavior, we identify key scientific goals and team processes that occur in each phase and across multiple phases. We then provide real-world exemplars for each phase that highlight strategies for successfully meeting the goals and engaging in the team processes that are hallmarks of that phase. We conclude by discussing the relevance of the model for TD team-based research initiatives, funding to support these initiatives, and future empirical research that aims to better understand the processes and outcomes of TD team-based research.},
  file = {C\:\\Users\\vanlissa\\Zotero\\storage\\5J8HEVQY\\Hall et al. - 2012 - A four-phase model of transdisciplinary team-based.pdf;C\:\\Users\\vanlissa\\Zotero\\storage\\5NJVDSK3\\4563078.html}
}

@article{hallMovingScienceTeam2008,
  title = {Moving the {{Science}} of {{Team Science Forward}}: {{Collaboration}} and {{Creativity}}},
  shorttitle = {Moving the {{Science}} of {{Team Science Forward}}},
  author = {Hall, Kara L. and Feng, Annie X. and Moser, Richard P. and Stokols, Daniel and Taylor, Brandie K.},
  date = {2008-08-01},
