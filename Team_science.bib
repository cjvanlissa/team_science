@article{koeslag-kreunenWhenLeadershipPowers2018,
  title = {When {{Leadership Powers Team Learning}}: {{A Meta-Analysis}}},
  shorttitle = {When {{Leadership Powers Team Learning}}},
  author = {Koeslag-Kreunen, Mieke and Van den Bossche, Piet and Hoven, Michael and Van der Klink, Marcel and Gijselaers, Wim},
  date = {2018-08-01},
  journaltitle = {Small Group Research},
  volume = {49},
  number = {4},
  pages = {475--513},
  publisher = {{SAGE Publications Inc}},
  issn = {1046-4964},
  doi = {10.1177/1046496418764824},
  url = {https://doi.org/10.1177/1046496418764824},
  urldate = {2023-12-02},
  abstract = {Team learning behavior is found to be one of the most effective team processes, as learning behavior at the team level (e.g., sharing, discussing, and reflecting on knowledge and actions) enables teams to adapt existing or develop new knowledge. Team leadership behavior is considered a critical accelerant for creating conditions that are essential to engage in team learning behavior, such as a safe environment. Yet despite the growing amount of research in team learning, this relationship remains unclear. Meta-analytic techniques were used to examine when team leadership behaviors support team learning behavior and how the task type moderates that relationship. Forty-three empirical studies reporting 92 effect sizes were synthesized. Analyses show that team leadership behavior explains 18\% of the variance in team learning behavior. Furthermore, results indicate that person-focused leaders foster team learning for both adaptive and developmental tasks, whereas task-focused leaders influence team learning for adaptive tasks only.},
  issue = {4},
  langid = {english}
}

