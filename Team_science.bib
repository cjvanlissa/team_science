  langid = {english},
  keywords = {facilitated consensus process,good practices,practice-based research networks,research practices,sense-making,team science}
}

@article{cannonConvergenceHowTransdisciplinary2020,
  title = {Towards {{Convergence}}: {{How}} to {{Do Transdisciplinary Environmental Health Disparities Research}}},
  shorttitle = {Towards {{Convergence}}},
  author = {Cannon, Clare E. B.},
  date = {2020-01},
  journaltitle = {International Journal of Environmental Research and Public Health},
  volume = {17},
