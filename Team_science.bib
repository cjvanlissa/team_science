  author = {Hoffmann, Sabine and Deutsch, Lisa and Klein, Julie Thompson and O’Rourke, Michael},
  date = {2022-04-25},
  journaltitle = {Humanities and Social Sciences Communications},
  shortjournal = {Humanit Soc Sci Commun},
  volume = {9},
  number = {1},
